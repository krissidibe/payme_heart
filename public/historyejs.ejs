<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <script src="https://cdn.tailwindcss.com"></script>
    <style>
      @import
      url("https://fonts.googleapis.com/css2?family=Londrina+Solid:wght@100;300;400;900&display=swap");
    </style>
    <script src="history.js"></script>
  </head>
  <body id="template" class="">
    <!-- info -->
    <div
      class="h-[125px] w-full flex justify-between items-end p-7 bg-[#23201F] text-white"
    >
      <div class="">
        <p class="text-[11px]">www.meentreprisegroup.com</p>
      </div>
      <div class="">
    
        <p class="text-[11px]">Notre adresse : <%=data.enterprise.address%></p> 
        <p class="text-[11px]">Tel :<%=data.enterprise.indicatif%> <%=data.enterprise.numberPrimary%> /  <%=data.enterprise.numberSecondary%></p>
        <p class="text-[11px]">E-mail : <%=data.enterprise.email%></p>

        <p class="text-[11px]"><%=data.enterprise.website%></p>
      </div>

    </div>

    <div
      class="flex justify-end w-full mt-8 mb-6 text-2xl font-bold px-7"
    >RELEVÉS D'OPÉRATIONS</div>

    <div class="px-7 mt-[0px]">
      <div
        class="flex justify-between w-full mb-2 text-[11px] text-white py-3 font-bold bg-[#23201F] px-7"
      >
        <p>HISTORIQUE DES FINANCES </p>
        <p><%=data.startAt%> AU <%=data.endAt%></p>

      </div>
      <div
        class="flex text-[13px] font-bold text-black bg-white border-b-2 border-t-2 border-black"
      >
        <div
          class="py-3 text-[11px] border-white min-w-[104px] text-center uppercase"
        >
          DATE
        </div>
        <div
          class="py-3 text-[11px] border-white min-w-[110px] text-center uppercase"
        >
          CLIENT
        </div>
        <div
          class="py-3 text-[11px] border-white min-w-[130px] text-center uppercase"
        >
          PROJET
        </div>
        <div
          class="w-full px-6 py-3 text-[11px] text-center uppercase max-w-[100px]"
        >TAXE</div>
        <div
          class="w-full px-6 py-3 text-[11px] text-left uppercase"
        >MONTANT</div>
        <div
          class="w-full px-6 py-3 text-[11px] text-left min-w-[180px] uppercase"
        >TYPE DE VERSEMENT</div>
      </div>
       

      <% data.dataTable.forEach((item,index) => { %>
       


        <% if (index % 2 != 0) { %>
          <div class="flex text-[13px] font-semibold text-black  bg-gray-100">
          <% } %>
          <% if (index % 2 == 0) { %>
            <div class="flex text-[13px] font-semibold text-black  ">
            <% } %>
      

   
        <div class="py-3 text-[11px] border-white min-w-[104px] text-center   ">
          

         
             
             <%=item.createdAt%>
          
        </div>
        <div
          class="py-3 text-[11px] line-clamp-1 max-h-[30px] border-white min-w-[110px] text-center"
        >
            
          <%=item.client%>    
        </div>
        <div
          class="py-3 text-[11px] line-clamp-1 ] border-white min-w-[130px] max-h-[130px] text-center"
        >
           <%=item.projectName%>  
        </div>
        <div
          class="w-full px-6 py-3 text-[11px] text-center max-w-[100px]"
        >  <%=item.taxe%>  </div>
        <div class="w-full px-6 py-3 text-[11px] text-left"> 
          
      
          <%=     parseInt(item.amountTotal).toString().replace(/\B(?<!\.\d*)(?=(\d{3})+(?!\d))/g, ".") %>   </div>
        <div
          class="w-full px-6 py-3 text-[11px] text-left   min-w-[180px]"
        ><%=item.type%></div>
      </div>
   
   
<%  }); %>
     



      
      

    </div>

    <div class="flex justify-end mt-10 px-7">
      <p class="mt-4 text-[13px] font-bold bg-black text-white p-2 px-3 flex">
        TOTAL : 39.060.000 FCFA
      </p>
    </div>
  </body>
  <script   type="text/x-handlebars-template">
  Handlebars.registerHelper('isDivisor', function (num1) {
    return "kris";
});
</script>
</html>