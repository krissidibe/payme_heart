    <img class="object-left object-contain h-[120px] w-[400px]" src="<%=logo%>" alt="" srcset="">
    <div class="items-center flex-1 hidden heading-info">
      <p class="text-[12px]">
        <%= dataEnterpriseInfo %> 
      </p>
    </div>
  </div>
  
  <div class="headline inter leading-5 font-light text-[16.4px]">
    <p>Objet: <%= project.name %></p>
