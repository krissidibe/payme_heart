<!DOCTYPE html>
<html lang="en">
  <head>
    <script src="https://cdn.tailwindcss.com"></script>
  </head>

  <style>
    * {
      margin: 0;
      padding: 0;
      box-sizing: border-box;
    }
    @import url('https://fonts.googleapis.com/css2?family=Poppins:wght@400;600&family=Inter:wght@400;600&display=swap');

    body {
      font-family: 'Poppins', sans-serif;;
    }
    .inter {
      font-family: 'Inter', sans-serif;
    }
    .poppin {
      font-family: 'Poppins', sans-serif;;
    }
    .page-header,
    .page-header-space {
      height: 100px;
    }

    .page-footer,
    .page-footer-space {
      height: 50px;
    }

    .page-footer {
      position: fixed;
      bottom: 80px;

      width: 100%;
      /* for demo */
      /* for demo */
    }

    .page-header {
      position: fixed;
      top: 0mm;
      width: 100%;
      border-bottom: 1px solid black; /* for demo */
      background: yellow; /* for demo */
    }

    .page {
      page-break-after: always;
      padding-bottom: 0px;
    }

    @page {
      margin: 0mm;
    }

    @media print {
      thead {
        display: table-header-group;
      }
      tfoot {
        display: table-footer-group;
      }

      button {
        display: none;
      }

      body {
        margin: 0;
      }
    }
    /* Facture Custom styling */
    @font-face {
      font-family: 'Helvetica Neue';
      src: url('path-to-your-fonts/HelveticaNeue-Bold.woff2') format('woff2'),
          url('path-to-your-fonts/HelveticaNeue-Bold.woff') format('woff');
      font-weight: bold;
      font-style: normal;
    }

    .heading {
      margin-top: 110px;
    }

    .facture-number div p:first-child {
      color: #010101;
    }

    .pages .facture-number {
      padding-right: 0;
      border-color: #010101;
    }

    .facture-number .dividor {
      display: block !important;
      width: 80px;
      border-top: 1px solid #010101;
      margin-left: 11px;
      margin-right: 3px;
    }

    .heading .logo {
      font-family: 'Helvetica Neue', sans-serif;
      background-color: #C03432;
      color: #383838;
    }

    .heading .logo p {
      margin-left: 0;
    }

    .headline {
      padding-top: 44px;
      border-color: #C03432;
    }

    .headline p {
      font-size: 13.4px;
      font-weight: 400;
    }

    .pages .tabs {
      margin-top: 12px;
      padding-bottom: 0;
    }

    .tabs-item {
      color: #4D4B4C;
      border-bottom: 1px solid #010101;
    }


    .tabs .tabs-header {
      color: #222428;
      font-weight: 700;
      padding-top: 0 !important;
      border: 1px solid #C03432;
    }

    .tabs .tabs-header p {
      padding-bottom: 0;
    }


    .tabs .tabs-header p:first-child {
      background: transparent;
    }

    .total {
      background: transparent;
      margin-left: 56px;
      margin-right: 56px;
      padding-top: 25px;
    }

    .total > div {
      padding-bottom: 22px;
      border: none;
    }

    .total .total-amount {
      background: #C03432;
      color: white;
    }

    .sign {
      margin-top: 60px;
      font-weight: 700;
      
    }

    .footer {
      padding-bottom: 26px;
      bottom: -50px !important;
      z-index: 1;
      color: #383838;
      margin-bottom: 42px;
    }

    .footer-mark {
      width: 229.3px;
      height: 18px;
      position: absolute;
      bottom: 8px;
      left: 0;
      background: #383838;
    }

    .footer-text {
      margin-left: 248px;
    }

    /* End Facture Custom styling */

    /* Validation styles */
    .pages {

      .tabs {
        .tabs-item {
          align-items: start;
        }
        
        .tabs-item p {
          padding-top: 0;
          padding-bottom: 0;
        }

        .tabs-item p:first-child {
          text-align: start;
          padding-left: 20px;
          width: 70px;
        }
        .tabs-item p:last-child {
          text-align: end;
          padding-right: 20px;
        }

        .tabs-header {
          margin-bottom: 10px;
        }

        .tabs-header p {
          text-align: center;
          padding-top: 6px;
          padding-bottom: 6px;
        }

        .tabs-header p:nth-child(2) {
          text-align: start;

      }
      
      }
      .total {
        padding-left: 20px;
        padding-right: 20px;
        div .right {
          .total-amount {
            display: flex;
            align-items: center;
            justify-content: space-between;
          }

          .total-amount span:last-child {
            font-size: 11px;
            padding: 0;
          }
        };
      }

      .sign {
        margin-right: 0px;
        margin-top: 20px;
      }
  }
    /* End Validation styles */
  </style>

