    /* Facture Custom styling */
    .heading {
      padding: 0 70px;
      margin: 46.9px 0 0 0;

      .logo {
        font-family: 'Helvetica Neue', sans-serif;
        background-color: #ECB05D;
        color: #383838;
      }

      .logo p {
        margin-left: 0;
      }

      .facture-number {
        padding: 0 20px;
        border-left: 4px solid #D24635;
        border-right: 4px solid #D24635;
      }
    }

    .headline {
      padding: 0 70px;
      margin-top: 54.15px;

      > div:first-child {
        padding-right: 21.18px;
        margin-left: 51px;
      }

      > div:nth-child(2) {
        padding-left: 9px;
      }

      > div:last-child {
        padding-left: 23px;
        margin-right: 63px;
      }
    }

    .tabs {
      margin-top: 63px;
      padding: 14px 56px;

      .tabs-header {
        margin-bottom: 12px;
        font-size: 10px !important;
        color: #222428;
        font-weight: 400;
        background: transparent;
        border-top: 1px solid #4D4C4C;
        border-bottom: 1px solid #4D4C4C;
      }

      .tabs-header p:first-child {
        background: transparent;
      }

      .tabs-item {
        padding: 12px 16px;
        color: #4D4B4C;
      }
      
      .tabs-item:last-child {
        padding-bottom: 24px;
        border-bottom: 1px solid #4D4C4C;
      }
      
      .tabs-item:nth-child(even) {
        background: #F4F4F4;
      }

    } 


    .total {
      margin: 20px 56px 0 56px;
      padding: 20px 16px 0 16px;
      > div {
        padding-bottom: 22px;
      }

      .right {
        padding-left: 45px;
        > p {
          margin: 0 ;
          margin-bottom: 12px;
        }

        > p.total-amount {
          color: black;
          margin-top: 22px;
          border-left: 4px solid #D24635;
          border-right: 4px solid #D24635;
          background-color: #FBEEED;
        }
      } 
      
    }

    .sign {
      padding-left: 72px;
      padding-right: 54px;
      font-weight: 700;
    }
    
    .footer {
      bottom: -20px !important;
      z-index: 1;
      color: #383838;
      margin-bottom: 10px;
    }

    .footer-mark {
      width: 229.3px;
      height: 18px;
      position: absolute;
      bottom: 8px;
      left: 0;
      background: #383838;
    }

    .footer-text {
      margin-left: 248px;
    }


