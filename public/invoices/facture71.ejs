<!DOCTYPE html>
<html lang="en">
  <head>
    <script src="https://cdn.tailwindcss.com"></script>
  </head>

  <style>
    :root
    * {
      margin: 0;
      padding: 0;
      box-sizing: border-box;
    }
    @import url('https://fonts.googleapis.com/css2?family=Poppins:wght@400;600&family=Inter:wght@400;600&display=swap');

    body {
      font-family: 'Poppins', sans-serif;;
      background-color: #fff;
    }
    .inter {
      font-family: 'Inter', sans-serif;
    }
    .poppin {
      font-family: 'Poppins', sans-serif;;
    }
    .page-header,
    .page-header-space {
      height: 100px;
    }

    .page-footer,
    .page-footer-space {
      height: 80px;
    }

    .page-footer {
      position: fixed;
      bottom: 80px;

      width: 100%;
      /* for demo */
      /* for demo */
    }

    .page-header {
      position: fixed;
      top: 0mm;
      width: 100%;
      border-bottom: 1px solid black; /* for demo */
      background: yellow; /* for demo */
    }

    .page {
      page-break-after: always;
      padding-bottom: 0px;
    }

    @page {
      margin: 0mm;
    }

    @media print {
      thead {
        display: table-header-group;
      }
      tfoot {
        display: table-footer-group;
      }

      button {
        display: none;
      }

      body {
        margin: 0;
       
      }
    }
    /* Facture Custom styling */
    @font-face {
      font-family: 'Helvetica Neue';
      src: url('path-to-your-fonts/HelveticaNeue-Bold.woff2') format('woff2'),
          url('path-to-your-fonts/HelveticaNeue-Bold.woff') format('woff');
      font-weight: bold;
      font-style: normal;
    }
    .heading {
      margin-top: 110px;
    }

    .facture-number div p:first-child {
      color: #010101;
    }

    .facture-number div p:first-child {
      color: #010101;
    
    }

    .facture-number {
      border-color: #010101;
      font-size: 16px !important;
    }

    .facture-number .dividor {
      display: block !important;
      width: 50px;
      border-top: 1px solid #010101;
      margin-left: 11px;
      margin-right: 3px;
    }
    .facture-number .dividor2 {
    
      display: block !important;
      width: 73px;
      border-top: 1px solid #010101;
      margin-left: 11px;
      margin-right: 3px;
      
     
    }

    .heading .logo {
      font-family: 'Helvetica Neue', sans-serif;
      background-color: transparent;
      color: #383838;
    }

    .pages .heading .logo p {
      margin-left: 30px;
    }

    .pages .headline {
      margin-top: 40px !important;
    }

    .headline {
      margin-top: 30.5px !important;
      padding-top: 0px;
  
    }

    .pages .tabs {
      margin-top: 35px;
      margin-left: 56px !important;
      margin-right: 56px !important;
    
   
    }

    .tabs-item {
      color: #4D4B4C;
      padding: 0px !important;
    }

    .tabs-item:first-child p {
      padding-top: 16px !important;
      
       height: 50px !important;
    }

    .tabs-item:last-child p{
      padding-bottom: 36px;
    }

    .tabs .tabs-header {
      color: #3C3C3B;
      font-weight: 700;
    }

    .tabs .tabs-header p {
      background: transparent;
      padding-bottom: 0;
    }

    .tabs .tabs-header p:first-child {
      padding-left: 0;
      background: transparent;
    }

    .pages .total {
      margin-top:10px !important;
      border-bottom: none;
      padding-bottom: 5px;
      padding-left: 56px;
    }
    *{
      border-right: none !important;
    }

     .totalborder {
      
   
    width: 350px;
     bottom: -2px;
     position: absolute;
     right: 45px;
     display: none;
  
     border-bottom: 4px solid var(--primary-color,#CA9E67) ;
    
    }

    .total .right {
      position: relative;
      bottom: -2px;
      gap: 0px !important;
    }
    .total .left  {
       
      padding-top: 18px;
     }

    .total .right p:last-child {

      border-top: 1px solid #010101;
    
    }

    .total .total-amount {
      color: #383838;
    }

    .sign {
      font-weight: 700;
    }

    .signature {
      margin-top: 0px !important;
      font-weight: 700;
    }
    
    .footer {
      bottom: 0;
      color: #383838; 
      display: flex !important;
       
    }

    .footer > p {
      
      padding: 0px 10px !important;
      padding-left: 20px !important;
      color: #383838; 
      display: flex !important;
       
    }

    .footer-mark {
      
      width: 234px;
      height: 20px;
      position: relative;
      bottom: -4px;
      left: 0;
      display: none;
      background: #383838;
    }

    .footer-mark-last {
      
      width: 234px;
      height: 20px;
      position: relative;
      bottom: -4px;
      left: 0;
      background: #383838;
     
    }

 

    .footer-img {
      height: 278px;
      width: 278px;
      position: fixed;
      bottom: 0 !important;
      right: -16px;
    }

    /* End Facture Custom styling */
  </style>

  <body>
    <!-- footer -->
    <div class="bottom-0 flex items-center justify-center py-2 pt-0 footer page-footer inter">
      <div class="footer-mark-last min-w-[180px] hidden ">
      </div>
      <div class="bg-black w-[250px] h-[15px] "></div>
      <p class="text-[11px]  px-[57px]  text-left line-clamp-3">
      
        <%= dataEnterpriseInfo %> 
        <!--   <%= enterprise.name %> - notre adresse <%= enterprise.address %> - Rccm :  <%= enterprise.rccm %> - Email :  <%= enterprise.email %> - Compte Bank :  <%= enterprise.bankNumber %> - TÃ©l : +<%= JSON.parse(enterprise.numbers)[0].indicatif %><%= JSON.parse(enterprise.numbers)[0].number %> / +<%= JSON.parse(enterprise.numbers)[1]?.indicatif %><%= JSON.parse(enterprise.numbers)[1]?.number %> - <%= JSON.parse(enterprise.nif)[0].content %> <%= JSON.parse(enterprise.nif)[1]?.content %>
       -->
      </p>
      <div class="footer-mark min-w-[180px] ">
      </div>
    
    </div>
    <table>
      <tbody>
        <tr>
          <td class="pages w-[827px]">

            <div class="flex flex-col w-full page-content">
              <!-- logo + facture -->
              <%- include('../subtemplates/header4') %>
          
              <!-- headline (object, address, client) -->
              <%- include('../subtemplates/headline1') %>

              <!-- table -->
              <%- include('../subtemplates/table2') %>
                
              <!-- facture totals informations -->
              <%- include('../subtemplates/total2') %>


              <!-- signature -->
              <%- include('../subtemplates/sign1') %>

              <!-- footer -->
            </div>
          </td>
        </tr>
      </tbody>

      <tfoot>
        <tr>
          <td>
            <!--place holder for the fixed-position footer-->
            <div class="page-footer-space"></div>
          </td>
        </tr>
      </tfoot>
    </table>
  </body>
</html>
